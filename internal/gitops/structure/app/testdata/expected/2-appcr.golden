apiVersion: application.giantswarm.io/v1alpha1
kind: App
metadata:
  labels:
    giantswarm.io/cluster: demowc
  name: demowc-hello-world
  namespace: org-demoorg
spec:
  catalog: giantswarm
  name: hello-world
  namespace: default
  version: 0.3.0
  kubeConfig:
    context:
      name: demowc
    inCluster: false
    secret:
      name: demowc-kubeconfig
      namespace: org-demoorg
  userConfig:
    secret:
      name: demowc-hello-world-user-values
      namespace: org-demoorg
